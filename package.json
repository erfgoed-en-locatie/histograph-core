{
  "name": "histograph-core",
  "version": "0.5.1",
  "description": "Read from Redis and index to Graphmalizer and Elasticsearch",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jshint index.js && jscs index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/histograph/core.git"
  },
  "author": "Jelle 'wires' Herold <jelle@waag.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/histograph/core/issues"
  },
  "homepage": "https://github.com/histograph/core",
  "dependencies": {
    "elasticsearch": "^12.1.3",
    "highland": "^2.5.1",
    "fuzzy-dates": "git+https://github.com/histograph/fuzzy-dates#puppetdeploy",
    "graphmalizer-core": "git+https://github.com/graphmalizer/graphmalizer-core#puppetdeploy",
    "histograph-config": "git+https://github.com/histograph/config#puppetdeploy",
    "histograph-schemas": "git+https://github.com/histograph/schemas#puppetdeploy",
    "histograph-uri-normalizer": "git+https://github.com/histograph/uri-normalizer#puppetdeploy",
    "minimist": "^1.1.3",
    "redis": "^2.6.3"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "jscs": "^3.0.7",
    "jshint": "^2.6.0"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": false,
    "undef": true,
    "unused": true,
    "noarg": true
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "disallowMultipleVarDecl": true,
    "requireMultipleVarDecl": null
  }
}
